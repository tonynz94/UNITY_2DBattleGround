<?xml version="1.0" encoding="utf-8" ?>
<PDL>

  <packet name="S_HandShake">
    <int name="CGUID"/>
  </packet>
  
  <packet name="C_FirstEnter">
    <String name="playerNickName"/>
  </packet>
  
  <packet name="S_FirstEnter">
    <String name="playerNickName"/>
    <int name="CGUID"/>
  </packet>

  <packet name="S_AllPlayerList">
      <list name="OnLinePlayer">
          <int name="CGUID"/>
          <string name="playerNickName"/>
          <int name="Level"/>
      </list>
  </packet>

  <packet name="C_CreateGameRoom">
    <int name="CGUID"/>
    <int name="MapType"/>
    <int name="GameType"/>
  </packet>

  <packet name="S_CreateGameRoom">
    <int name="CGUID"/>
    <int name="MapType"/>
    <int name="GameType"/>
    <int name="RoomId"/>
  </packet>

  <packet name="C_GetGameRooms">
    <int name="result"/>
  </packet>

  <packet name="S_GetGameRooms"> 
    <list name="GameRoomlist">
      <int name="RoomId"/>
      <int name="RoomOwner"/>
      <int name="MapType"/>
      <int name="GameMode"/>
      <list name="PlayerList">
        <int name="CGUID"/>
      </list>
    </list>
  </packet>


  <packet name="C_IntroToLobby">
    <int name="CGUID"/>
  </packet>

  <packet name="C_LobbyToGame">
    <int name="CGUID"/>
    <int name="roomId"/>
  </packet>

  <packet name="C_ClickReadyOnOff">
    <bool name="isReady"/>
    <int name="roomId"/>
    <int name="CGUID"/>
  </packet>

  <packet name="S_ClickReadyOnOff">
    <bool name="isReady"/>
    <int name="roomId"/>
    <int name="CGUID"/>
  </packet>


  <packet name="S_LobbyToGame">
    <int name="CGUID"/>
    <int name="roomId"/>
    <bool name="IsPlayerEntered"/>
  </packet>

  <packet name="C_GameToLobby">
    <int name="CGUID"/>
    <int name="roomId"/>
  </packet>

  <packet name="S_GameToLobby">
    <int name="CGUID"/>
    <int name="roomId"/>
    <int name="newOwner"/>
  </packet>

  <packet name="C_SendChat">
    <int name="messageType"/>
    <string name="nickName"/>
    <string name="chatContent"/>
  </packet>

  <packet name="S_SendChat">
    <int name="messageType"/>
    <string name="nickName"/>
    <string name="chatContent"/>
  </packet>

  <packet name="S_NoticeAll">
    <string name="chatContent"/>
  </packet>
  
  <packet name="S_BroadcastEnterGame">
	<int name="playerId"/>
	<float name="posX"/>
	<float name="posY"/>
	<float name="posZ"/>
  </packet>

  <packet name="S_BroadcastLeaveGame">
    <int name="playerId"/>
  </packet>
  
  <packet name="C_LeaveGame">
  </packet>

  <packet name="S_GameRoomCountDownStart">
  </packet>

  <packet name="S_GameStart">
  </packet>
 

  <packet name="C_EnterFieldWorld">
  </packet>
  
  <packet name="S_EnterFieldWorld">
  </packet>
  
  <packet name="S_PlayerList">
	<list name="player">
	  <bool name="isSelf"/>
	  <int name="playerId"/>
	  <float name="posX"/>
	  <float name="posY"/>
	  <float name="posZ"/>
	</list>
    
  </packet>
  <packet name="C_Move">
	<float name="posX"/>
	<float name="posY"/>
	<float name="posZ"/>
  </packet>
  
  <packet name="S_BroadcastMove">
	<int name="playerId"/>
	<float name="posX"/>
	<float name="posY"/>
	<float name="posZ"/>
  </packet>
  
</PDL>