<?xml version="1.0" encoding="utf-8" ?>
<PDL>

  <packet name="S_HandShake">
    <int name="CGUID"/>
  </packet>
  
  <packet name="C_FirstEnter">
    <String name="playerNickName"/>
  </packet>
  
  <packet name="S_FirstEnter">
    <String name="playerNickName"/>
    <int name="CGUID"/>
  </packet>

  <packet name="C_SendChat">
    <int name="messageType"/>
    <string name="nickName"/>
    <string name="chatContent"/>
  </packet>

  <packet name="S_SendChat">
    <int name="messageType"/>
    <string name="nickName"/>
    <string name="chatContent"/>
  </packet>

  <packet name="S_NoticeAll">
    <string name="chatContent"/>
  </packet>
  
  <packet name="S_BroadcastEnterGame">
	<int name="playerId"/>
	<float name="posX"/>
	<float name="posY"/>
	<float name="posZ"/>
  </packet>
  <packet name="C_LeaveGame">
  </packet>
  <packet name="S_BroadcastLeaveGame">
	<int name="playerId"/>
  </packet>
  <packet name="S_PlayerList">
	<list name="player">
	  <bool name="isSelf"/>
	  <int name="playerId"/>
	  <float name="posX"/>
	  <float name="posY"/>
	  <float name="posZ"/>
	</list>
  </packet>
  <packet name="C_Move">
	<float name="posX"/>
	<float name="posY"/>
	<float name="posZ"/>
  </packet>
  <packet name="S_BroadcastMove">
	<int name="playerId"/>
	<float name="posX"/>
	<float name="posY"/>
	<float name="posZ"/>
  </packet>
</PDL>