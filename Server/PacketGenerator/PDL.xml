<?xml version="1.0" encoding="utf-8" ?>
<PDL>

  <packet name="S_HandShake">
    <int name="CGUID"/>
  </packet>
  
  <packet name="C_FirstEnter">
    <String name="playerNickName"/>
  </packet>
  
  <packet name="S_FirstEnter">
    <String name="playerNickName"/>
    <int name="CGUID"/>
  </packet>

  <packet name="S_AllPlayerList">
      <list name="OnLinePlayer">
          <int name="CGUID"/>
          <string name="playerNickName"/>
          <int name="Level"/>
      </list>
  </packet>

  <packet name="C_CreateGameRoom">
    <int name="CGUID"/>
    <int name="MapType"/>
    <int name="GameType"/>
  </packet>

  <packet name="S_CreateGameRoom">
    <int name="CGUID"/>
    <int name="MapType"/>
    <int name="GameType"/>
    <int name="RoomId"/>
  </packet>

  <packet name="C_GetGameRooms">
    <int name="result"/>
  </packet>

  <packet name="S_GetGameRooms"> 
    <list name="GameRoomlist">
      <int name="RoomId"/>
      <int name="RoomOwner"/>
      <int name="MapType"/>
      <int name="GameMode"/>
      <bool name ="isStarted"/>
      <list name="PlayerList">
        <int name="CGUID"/>
        <bool name="isReady"/>
      </list>
    </list>
  </packet>

  <packet name="C_IntroToLobby">
    <int name="CGUID"/>
  </packet>

  <packet name="C_LobbyToGame">
    <int name="CGUID"/>
    <int name="roomId"/>
  </packet>

  <packet name="C_ClickReadyOnOff">
    <bool name="isReady"/>
    <int name="roomId"/>
    <int name="CGUID"/>
  </packet>

  <packet name="S_ClickReadyOnOff">
    <bool name="isReady"/>
    <int name="roomId"/>
    <int name="CGUID"/>
  </packet>

  <packet name="S_LobbyToGame">
    <int name="CGUID"/>
    <int name="roomId"/>
    <bool name="IsPlayerEntered"/>
  </packet>

  <packet name="C_GameToLobby">
    <int name="CGUID"/>
    <int name="roomId"/>
  </packet>

  <packet name="S_GameToLobby">
    <int name="CGUID"/>
    <int name="roomId"/>
    <int name="newOwner"/>
  </packet>

  <packet name="C_SendChat">
    <int name="messageType"/>
    <string name="nickName"/>
    <string name="chatContent"/>
  </packet>

  <packet name="S_SendChat">
    <int name="messageType"/>
    <string name="nickName"/>
    <string name="chatContent"/>
  </packet>

  <packet name="S_NoticeAll">
    <string name="chatContent"/>
  </packet>
  
  <packet name="S_BroadcastEnterGame">
	<int name="playerId"/>
	<float name="posX"/>
	<float name="posY"/>
	<float name="posZ"/>
  </packet>

  <packet name="S_BroadcastLeaveGame">
    <int name="playerId"/>
  </packet>
  
  <packet name="C_LeaveGame">
  </packet>

  <packet name="C_GameStart">
    <int name="CGUID"/>
    <int name="roomID"/>
  </packet>

  <packet name="S_GameStart">
    <bool name="isStart"/>
    <int name="roomID"/>
  </packet>
 
  <packet name="C_EnterFieldWorld">
    <int name ="CGUID"/>
    <int name ="roomID"/>
  </packet>
  
  <packet name="S_EnterFieldWorld">
    <int name ="CGUID"/>
    <int name="posX"/>
    <int name="posY"/>
    <int name="posZ"/>
  </packet>
  
  <packet name="S_PlayerList">
	<list name="player">
	  <bool name="isSelf"/>
	  <int name="playerId"/>
	  <float name="posX"/>
	  <float name="posY"/>
	  <float name="posZ"/>
	</list>
  </packet>

  <packet name="C_Move">
    <int name="roomID"/>
    <int name="CGUID"/>
	  <float name="posX"/>
	  <float name="posY"/>
    <int name ="cellPosX"/>
    <int name ="cellPosY"/>
    <int name="Dir"/>
    <int name ="State"/>
  </packet>
  
  <packet name="S_BroadcastMove">
	  <int name="CGUID"/>
	  <float name="posX"/>
	  <float name="posY"/>
    <int name ="cellPosX"/>
    <int name ="cellPosY"/>
    <int name="Dir"/>
    <int name ="State"/>
  </packet>

  <packet name="C_WaterBOOM">
    <int name ="roomID"/>
    <int name = "CGUID"/>
    <int name ="CellPosX"/>
    <int name ="CellPosY"/>
  </packet>

  <packet name="S_WaterBOOM">
    <int name ="CGUID"/>
    <int name ="ID"/>
    <int name ="CellPosX"/>
    <int name ="CellPosY"/>
  </packet>

  <packet name="S_WaterBlowUp">
    <int name ="ID"/>
  </packet>

  <packet name="S_PlayerHit">
    <int name="CGUID"/>
    <int name="Damage"/>
    <int name="HitPlayerLeftHP"/>
  </packet>
  
  <packet name="S_PlayerDie">
    <int name="CGUID"/>
    <int name="AttackerCGUID"/>
    <int name="Damage"/>
  </packet>

  <packet name="C_GameFinish">
    <int name="roomID"/>
  </packet>

  <packet name="C_FieldToLobby">
    <int name="CGUID"/>
    <int name="RoomID"/>
  </packet>

  <packet name="C_SkillState">
    <int name="CGUID" />
    <int name="SpeedUpPoint"/>
    <int name="RangeUpPoint"/>
    <int name="PowerUpPoint"/>
    <int name="WaterCountUpPoint"/>
  </packet>

  <packet name="S_SkillState">
    <int name="Speed"/>
  </packet>
</PDL>